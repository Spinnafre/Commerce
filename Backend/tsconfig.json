{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    // "rootDir": "./",
    // "baseUrl": "./src",
    "outDir": "./dist",
    "removeComments": true,   
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  }
}